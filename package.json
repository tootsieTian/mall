{
  "name": "mall",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build"
  },
  "dependencies": {
    "amfe-flexible": "^2.2.1",
    "axios": "^0.21.1",
    "core-js": "^3.6.4",
    "mock": "^0.1.1",
    "mockjs": "^1.1.0",
    "swiper": "^5.3.7",
